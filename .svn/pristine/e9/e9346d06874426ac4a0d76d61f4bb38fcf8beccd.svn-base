/*--下拉--*/
#pullDown,
#pullUp { font-size: 14px; font-weight: bold; line-height: 40px; height: 40px; padding: 5px 10px; color: #888; }
#pullDown .pullDownIcon,
#pullUp .pullUpIcon { display: block; float: left; width: 40px; height: 40px; -webkit-transition-duration: 250ms; -webkit-transition-property: -webkit-transform; background: url(../images/pull-icon@2x.png) 0 0 no-repeat; -webkit-background-size: 40px 80px; background-size: 40px 80px; }
#pullDown .pullDownIcon { -webkit-transform: rotate(0deg) translateZ(0); }
#pullUp .pullUpIcon { -webkit-transform: rotate(-180deg) translateZ(0); }
#pullDown.flip .pullDownIcon { -webkit-transform: rotate(-180deg) translateZ(0); }
#pullUp.flip .pullUpIcon { -webkit-transform: rotate(0deg) translateZ(0); }
#pullDown.loading .pullDownIcon,
#pullUp.loading .pullUpIcon { -webkit-transition-duration: 0ms; -webkit-transform: rotate(0deg) translateZ(0); -webkit-animation-name: loading; -webkit-animation-duration: 2s; -webkit-animation-timing-function: linear; -webkit-animation-iteration-count: infinite; background-position: 0 100%; }
@-webkit-keyframes loading {
	from { -webkit-transform: rotate(0deg) translateZ(0); }
	to { -webkit-transform: rotate(360deg) translateZ(0); }
}
